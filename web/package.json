{
  "name": "octant",
  "description": "Octant is an extensible platform that provides modules that help developers better understand their Kubernetes resources.",
  "author": "The Octant Team",
  "version": "0.25.1",
  "homepage": "https://octant.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vmware-tanzu/octant.git"
  },
  "bugs": {
    "url": "https://github.com/vmware-tanzu/octant/issues"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node ./node_modules/@angular/cli/bin/ng build --configuration production --output-hashing=all",
    "build-electron": "npx tsc main.ts && npm run build -- --base-href ./",
    "test": "ng test --watch=false --browsers=ElectronDebug",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless --source-map=false",
    "lint": "prettier -c 'src/**/*.ts'",
    "eslint": "npx eslint --ignore-path .gitignore --ext .js,.ts .",
    "clean": "rimraf ./dist/octant/*",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "electron:serve": "wait-on http-get://localhost:7777 && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "npx tsc -p tsconfig.serve.json",
    "electron:local": "npx tsc main.ts && npx ng build --base-href ./ && npx electron --local .",
    "electron:build": "npm run build-electron && npx electron-builder -p never",
    "storybook": "start-storybook -p 6006 -s src/assets",
    "build-storybook": "node ./node_modules/@storybook/angular/bin/build.js build-storybook -c .storybook",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "rimraf ./node_modules/cytoscape-node-html-label/dist/cytoscape-node-html-label.d.ts"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.3",
    "@angular/common": "^16.0.3",
    "@angular/compiler": "^16.0.3",
    "@angular/core": "^16.0.3",
    "@angular/elements": "^16.0.3",
    "@angular/forms": "^16.0.3",
    "@angular/platform-browser": "^16.0.3",
    "@angular/platform-browser-dynamic": "^16.0.3",
    "@angular/router": "^16.0.3",
    "@cds/angular": "^6.4.2",
    "@cds/city": "^1.1.0",
    "@cds/core": "^6.4.2",
    "@clr/angular": "^15.4.0",
    "@clr/icons": "^13.0.2",
    "@clr/ui": "^15.4.0",
    "@materia-ui/ngx-monaco-editor": "^6.0.0",
    "@ng-select/ng-select": "^11.0.0",
    "@types/cytoscape": "^3.19.9",
    "@webcomponents/custom-elements": "^1.6.0",
    "angular-resizable-element": "^7.0.2",
    "ansi_up": "^5.2.1",
    "core-js": "^3.30.2",
    "cytoscape": "^3.25.0",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "cytoscape-node-html-label": "^1.2.2",
    "d3-graphviz": "^5.0.2",
    "d3-zoom": "^3.0.0",
    "dagre-d3": "^0.6.4",
    "electron-store": "^8.1.0",
    "get-port": "^6.1.2",
    "highlight.js": "^11.8.0",
    "jscodeshift": "^0.15.0",
    "jsoneditor": "^9.10.2",
    "karma-electron": "^7.3.0",
    "karma-electron-launcher": "^0.3.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.38.0",
    "ngx-highlightjs": "^9.0.0",
    "open": "^9.1.0",
    "rxjs": "^7.8.1",
    "serialize-javascript": "^6.0.1",
    "tslib": "^2.5.2",
    "ws": "^8.13.0",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.3",
